{
  "name": "yawn-yaml",
  "version": "1.3.3",
  "author": "Mohsen Azimi <me@azimi.me>",
  "license": "MIT",
  "description": "YAML parser that preserves comments and styling",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "posttest": "yarn lint",
    "prepublish": "tsc",
    "precommit": "lint-staged",
    "fix": "prettier --write '**/*.{ts,json,css,md}'",
    "lint": "prettier -l '**/*.{ts,json,css,md}'"
  },
  "lint-staged": {
    "*.{ts,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohsen1/yawn-yaml.git"
  },
  "keywords": [
    "yaml"
  ],
  "bugs": {
    "url": "https://github.com/mohsen1/yawn-yaml/issues"
  },
  "homepage": "https://github.com/mohsen1/yawn-yaml#readme",
  "dependencies": {
    "js-yaml": "^3.12.0",
    "lodash": "^4.17.10",
    "prettier": "prettier/prettier"
  },
  "devDependencies": {
    "@types/jest": "^23.1.1",
    "@types/js-yaml": "^3.11.1",
    "@types/lodash": "^4.14.110",
    "@types/node": "^10.3.4",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "lint-staged": "^7.2.0",
    "precommit": "^1.2.2",
    "ts-jest": "^22.4.6"
  }
}
